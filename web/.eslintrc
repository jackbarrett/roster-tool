{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {},
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest",
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "plugins": [],
  "rules": {
    "no-console": "off",
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "linebreak-style": "off",
    "no-unused-vars": ["warn"],
    "quotes": ["warn", "double"],
    "semi": ["error", "always"]
  }
}
